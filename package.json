{
  "name": "@standardnotes/stylekit",
  "version": "5.29.1",
  "main": "dist/stylekit.js",
  "types": "dist/stylekit.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "webpack --mode='production' && yarn run tsc",
    "start": "webpack --watch --mode='development'",
    "lint": "prettier --check *.js",
    "lint:fix": "prettier --write *.js",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -o storybook",
    "clean-cache:gh-pages": "rm -rf node_modules/.cache/gh-pages",
    "publish:storybook": "gh-pages -b gh-pages -d storybook",
    "release": "standard-version"
  },
  "dependencies": {
    "@nanostores/react": "^0.2.0",
    "@reach/listbox": "^0.17.0",
    "@reach/menu-button": "^0.17.0",
    "@standardnotes/icons": "^1.1.8",
    "nanoid": "^3.3.4",
    "nanostores": "^0.5.12",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-transform-react-jsx": "^7.17.12",
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@standardnotes/config": "^2.4.3",
    "@storybook/addon-a11y": "^6.5.5",
    "@storybook/addon-actions": "^6.5.5",
    "@storybook/addon-essentials": "^6.5.5",
    "@storybook/addon-links": "^6.5.5",
    "@storybook/builder-webpack5": "^6.5.5",
    "@storybook/manager-webpack5": "^6.5.5",
    "@storybook/react": "^6.5.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-loader": "^8.2.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.22.7",
    "css-loader": "~6.7.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "pre-push": "^0.1.2",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "sass-loader": "^13.0.0",
    "standard-version": "^9.5.0",
    "style-loader": "~3.3.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "pre-push": [
    "lint"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/standardnotes/StyleKit.git"
  },
  "peerDependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  }
}
