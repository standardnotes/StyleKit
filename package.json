{
  "name": "@standardnotes/stylekit",
  "version": "5.3.5",
  "main": "dist/stylekit.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "webpack --mode='production'",
    "start": "webpack --watch --mode='development'",
    "lint": "prettier --check *.js",
    "lint:fix": "prettier --write *.js",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -o storybook",
    "clean-cache:gh-pages": "rm -rf node_modules/.cache/gh-pages",
    "publish:storybook": "gh-pages -b gh-pages -d storybook",
    "release": "standard-version"
  },
  "dependencies": {
    "@reach/listbox": "^0.15.0",
    "@reach/menu-button": "^0.15.1",
    "@svgr/webpack": "^6.2.1",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-react-jsx": "^7.12.16",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@storybook/addon-a11y": "^6.1.17",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/preact": "^6.1.17",
    "babel-loader": "^8.2.2",
    "babel-preset-preact": "^2.0.0",
    "core-js": "^3.8.3",
    "css-loader": "~5.0.1",
    "gh-pages": "^3.1.0",
    "husky": "^7.0.4",
    "mini-css-extract-plugin": "^1.3.5",
    "node-sass": "^5.0.0",
    "pre-push": "^0.1.1",
    "preact": "^10.5.12",
    "preact-custom-element": "^4.2.1",
    "prettier": "2.2.1",
    "sass-loader": "^10.1.1",
    "standard-version": "^9.3.2",
    "style-loader": "~2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.20.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "preact": "^10.5.12"
  },
  "pre-push": [
    "lint"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/standardnotes/StyleKit.git"
  }
}
