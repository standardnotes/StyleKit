{
  "name": "@standardnotes/stylekit",
  "version": "5.15.0",
  "main": "dist/stylekit.js",
  "types": "dist/stylekit.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "webpack --mode='production' && yarn run tsc",
    "start": "webpack --watch --mode='development'",
    "lint": "prettier --check *.js",
    "lint:fix": "prettier --write *.js",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -o storybook",
    "clean-cache:gh-pages": "rm -rf node_modules/.cache/gh-pages",
    "publish:storybook": "gh-pages -b gh-pages -d storybook",
    "release": "standard-version"
  },
  "dependencies": {
    "@nanostores/preact": "^0.1.3",
    "@reach/listbox": "^0.16.2",
    "@reach/menu-button": "^0.16.2",
    "@svgr/webpack": "^6.2.1",
    "nanoid": "^3.3.1",
    "nanostores": "^0.5.10",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/preact": "^6.4.19",
    "babel-loader": "^8.2.3",
    "babel-preset-preact": "^2.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "core-js": "^3.21.1",
    "css-loader": "~6.6.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "mini-css-extract-plugin": "^2.5.3",
    "node-sass": "^7.0.1",
    "pre-push": "^0.1.1",
    "preact": "^10.6.6",
    "preact-custom-element": "^4.2.1",
    "prettier": "2.5.1",
    "sass-loader": "^12.6.0",
    "standard-version": "^9.3.2",
    "style-loader": "~3.3.1",
    "ts-loader": "^9.2.7",
    "typescript": "^4.6.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "peerDependencies": {
    "preact": "^10.6.6"
  },
  "pre-push": [
    "lint"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/standardnotes/StyleKit.git"
  }
}
